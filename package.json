{
  "name": "@stoffberg/create-t3svelte-app",
  "version": "1.0.0",
  "description": "Rapidly setup T3 Stack with SvelteKit",
  "exports": "./dist/index.js",
  "bin": {
    "create-t3svelte-app": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "compile": "tsup & node dist/build.cjs",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules",
    "start": "node dist/index.js",
    "lint": "eslint src --cache --cache-strategy content",
    "format": "prettier --write --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "format:check": "prettier --check --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "release": "changeset version",
    "pub:beta": "pnpm build && pnpm publish --tag beta",
    "pub:next": "pnpm build && pnpm publish --tag next",
    "pub:release": "pnpm build && pnpm publish"
  },
  "keywords": [
    "cli",
    "create-t3svelte-app"
  ],
  "author": "Dirk Stoffberg Beukes",
  "contributors": [
    {
      "name": "Dirk Stoffberg Beukes",
      "email": "dirk_beukes@hotmail.com",
      "url": "https://stoffberg.dev"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "kleur": "^4.1.5",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@prisma/client": "^4.8.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.5",
    "@trpc/client": "^10.8.1",
    "@trpc/server": "^10.8.1",
    "@types/gitignore-parser": "^0.0.0",
    "@types/prettier": "^2.7.2",
    "@types/prompts": "^2.4.1",
    "@typescript-eslint/parser": "^5.48.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "gitignore-parser": "^0.0.2",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "sucrase": "^3.29.0",
    "superjson": "^1.12.1",
    "svelte": "^3.55.0",
    "tailwindcss": "^3.2.4",
    "tiny-glob": "^0.2.9",
    "trpc-sveltekit": "^3.2.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "zod": "^3.20.2"
  }
}
